{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "8.11.1",
    "npm": "6.1.0"
  },
  "proxy": {
    "/api/*": {
      "target": "http://localhost:9080"
    }
  },
  "browserslist": {
    "production": [
      "last 1 version",
      "> 1%",
      "not dead",
      "ie >= 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie11"
    ]
  },
  "scripts": {
    "webpack-init": "webpack-dev-server --config=webpack.dev.js",
    "precommit": "lint-staged",
    "dev": "npm-run-all --parallel dev:*",
    "dev:client": "cross-env NODE_ENV=development webpack --config webpack.dev.js --watch",
    "dev:server": "cross-env NODE_ENV=development nodemon --inspect --watch config --watch server ./bin/www",
    "prod:client": "cross-env NODE_ENV=production webpack --config=webpack.prod.js --watch",
    "build:prod": "cross-env NODE_ENV=production webpack --config=webpack.prod.js",
    "start": "cross-env NODE_ENV=production node ./bin/www",
    "heroku-postbuild": "npm run build:prod",
    "configure": "./bin/configuredb.sh",
    "test": "cross-env NODE_ENV=test jest --coverage --watch",
    "test:client": "cross-env NODE_ENV=test jest --config ./client/jest.config.js --coverage --watch",
    "test:server": "jest --config ./server/jest.config.js --coverage --watch",
    "cypress:open": "cypress open",
    "test:e2e:dev": "npm-run-all --parallel dev:server cy:run",
    "test:e2e": "npm-run-all --parallel --race start cy:run",
    "cy:run": "cypress run",
    "coverage": "jest -u --maxWorkers=2 --coverage",
    "webpack-analyse": "cross-env NODE_ENV=test webpack --profile --json > stats.json",
    "predeploy": "npm run build:prod",
    "deploy": "gh-pages -d public/dist"
  },
  "lint-staged": {
    "linters": {
      "**/*.{js,jsx,json}": [
        "eslint --fix",
        "prettier --parser flow --write",
        "git add"
      ],
      "**/*.{scss}": [
        "prettier --parser flow --write",
        "git add"
      ]
    },
    "ignore": [
      ".*/dist",
      ".*/node_modules",
      "./package.json",
      "./package-lock.json",
      ".*/fixtures",
      "./cypress.json"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jin827/homemeal.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Jin827/homemeal/issues"
  },
  "homepage": "https://yu-chung.com/",
